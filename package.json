{
  "name": "blueprint-emulator",
  "version": "1.0.0",
  "description": "UE5.6教案自動生成システム - Unreal Engine 5.6 Lesson Plan Generator",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/cli/index.js",
    "dev": "tsx src/cli/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "generate": "tsx src/cli/index.ts generate",
    "info": "tsx src/cli/index.ts info"
  },
  "keywords": [
    "unreal-engine",
    "ue5",
    "education",
    "lesson-plan",
    "blueprint",
    "ai",
    "generator"
  ],
  "author": "BlueprintEmulator Team",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "dotenv": "^16.3.0",
    "openai": "^4.0.0",
    "ora": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
